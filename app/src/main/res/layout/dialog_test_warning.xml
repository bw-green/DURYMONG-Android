<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="342dp"
    android:layout_height="190dp">

    <TextView
        android:id="@+id/tv_test_warning_main"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="35dp"
        android:fontFamily="@font/suite_semi_bold"
        android:text="응답하지 않은 문항이 있어요!"
        android:textColor="@color/black"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_test_warning_context"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/suite_medium"
        android:text="미응답 시 결과를 알 수 없어요!"
        android:textColor="@color/gray3"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tv_test_warning_main" />

    <ImageView
        android:id="@+id/iv_test_warning_finish"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginBottom="16dp"
        android:contentDescription="끝내기"
        android:src="@drawable/img_result_dialog_back"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/suite_medium"
        android:text="끝내기"
        android:textColor="@color/main_purple"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="@id/iv_test_warning_finish"
        app:layout_constraintEnd_toEndOf="@id/iv_test_warning_finish"
        app:layout_constraintStart_toStartOf="@id/iv_test_warning_finish"
        app:layout_constraintTop_toTopOf="@id/iv_test_warning_finish" />

    <ImageView
        android:id="@+id/iv_test_warning_continue"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="16dp"
        android:contentDescription="답변하기"
        android:src="@drawable/img_result_dialog_ok"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/suite_medium"
        android:text="답변하기"
        android:textColor="@color/white"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="@id/iv_test_warning_continue"
        app:layout_constraintEnd_toEndOf="@id/iv_test_warning_continue"
        app:layout_constraintStart_toStartOf="@id/iv_test_warning_continue"
        app:layout_constraintTop_toTopOf="@id/iv_test_warning_continue" />


</androidx.constraintlayout.widget.ConstraintLayout>